    "@tauri-apps/api": "^1.5.1",
    "@types/d3": "^7.4.3",
    "@types/dompurify": "^3.0.5",
    "@types/uuid": "^10.0.0",
    "@visx/axis": "^3.12.0",
    "@visx/curve": "^3.12.0",
    "@visx/event": "^3.12.0",
    "@visx/grid": "^3.12.0",
    "@visx/group": "^3.12.0",
    "@visx/hierarchy": "^3.12.0",
    "@visx/pattern": "^3.12.0",
    "@visx/responsive": "^3.12.0",
    "@visx/scale": "^3.12.0",
    "@visx/shape": "^3.12.0",
    "@visx/text": "^3.12.0",
    "@visx/tooltip": "^3.12.0",
    "@visx/zoom": "^3.12.0",
    "@xyflow/react": "^12.9.2",
    "d3": "^7.9.0",
    "dompurify": "^3.3.0",
    "echarts": "^5.4.3",
    "echarts-for-react": "^3.0.2",
    "elkjs": "^0.11.0",
    "html-to-image": "^1.11.11",
    "jspdf": "^3.0.3",
    "lucide-react": "^0.525.0",
    "mermaid": "^11.12.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.5.0",
    "react-router-dom": "^6.20.1",
    "recharts": "^3.2.1",
    "uuid": "^13.0.0",
    "zustand": "^4.4.7"
